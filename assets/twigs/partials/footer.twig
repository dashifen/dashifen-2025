<footer id="site-footer" role="contentinfo">
  <div class="listening">
    <h3>Listening:</h3>
    <img src="{{ song.image }}" role="presentation" alt="">
    <p>
      <em>{{ song.track }}</em><br>
      by {{ song.artist }}
    </p>
  </div>

  <div class="reading">
    <h3>Reading:</h3>
    <img src="{{ books[0].image }}" role="presentation" alt="">
    <p>
      <em>{{ books[0].title }}</em><br>
      by {{ books[0].people[0].name }}
    </p>
  </div>

  <p class="copyright">
    Copyright &copy; {{ year }} David Dashifen Kees
  </p>
</footer>

{% if debug %}
  <!-- {{ page.context }} -->
{% endif %}
