{
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#1c1a22",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f9f9ed",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#cc95d0",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#00c4ff",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": "1100px"
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Nunito",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nunito/nunito-400-normal.woff2"
							]
						}
					],
					"fontFamily": "Nunito, sans-serif",
					"name": "Nunito",
					"slug": "nunito"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--base)",
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito)",
					"textDecoration": "underline"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--nunito)"
		}
	},
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}